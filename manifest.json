{
  "name": "progressive-discount-table",
  "vendor": "ssesandbox04",
  "version": "2.0.10",
  "title": "Progressive Discount Table",
  "description": "Renders a price by quantity table on products with progressive discount",
  "categories": [],
  "dependencies": {
    "vtex.product-context": "0.x",
    "vtex.format-currency": "0.x",
    "vtex.styleguide": "9.x",
    "vtex.css-handles": "0.x"
  },
  "builders": {
    "node": "6.x",
    "graphql": "1.x",
    "docs": "0.x",
    "react": "3.x",
    "store": "0.x",
    "messages": "1.x"
  },
  "policies": [
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "POWER_USER_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/{{account}}/pricing/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/rnb/pvt/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "kubernetes.default.svc.cluster.local",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "*",
        "path": "*"
      }
    },
    {
      "name": "Sku.aspx"
    },
    {
      "name": "read_prices"
    },
    {
      "name": "ADMIN_CRM"
    },
    {
      "name": "POWER_USER_CRM"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "*",
        "path": "*"
      }
    },
    {
      "name": "Sku.aspx"
    },
    {
      "name": "read_prices"
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "graphql-query"
    },
    {
      "name": "vbase-read-write"
    }
  ],
  "settingsSchema": {
    "title": "VTEX API Settings",
    "type": "object",
    "properties": {
      "X-VTEX-API-AppKey": {
        "type": "string"
      },
      "X-VTEX-API-AppToken": {
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
